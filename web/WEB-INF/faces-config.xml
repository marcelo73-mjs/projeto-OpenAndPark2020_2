<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaInformacoes</from-outcome>
            <to-view-id>/informacoes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaListagem</from-outcome>
            <to-view-id>/EstacionamentoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/informacoes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EstacionamentoListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaInclusao</from-outcome>
            <to-view-id>/EstacionamentoIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/EstacionamentoEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaDetalhe</from-outcome>
            <to-view-id>/EstacionamentoDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>reflesh</from-outcome>
            <to-view-id>/EstacionamentoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>estacionamentoController</managed-bean-name>
        <managed-bean-class>fja.edu.br.controladores.EstacionamentoController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/EstacionamentoIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/EstacionamentoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>cidadeController</managed-bean-name>
        <managed-bean-class>fja.edu.br.controladores.CidadeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/EstacionamentoEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/EstacionamentoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EstacionamentoDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/EstacionamentoListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/EstacionamentoEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>clienteController</managed-bean-name>
        <managed-bean-class>fja.edu.br.controladores.ClienteController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>comentarioController</managed-bean-name>
        <managed-bean-class>fja.edu.br.controladores.ComentarioController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>loginRepositorio</managed-bean-name>
        <managed-bean-class>fja.edu.br.infraestrutura.repositorio.LoginRepositorio</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/loginAdminIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaListagem</from-outcome>
            <to-view-id>/EstacionamentoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
